h2 It is currently {{ currentConditions }}

skycon(width=64, height=64, type="conditionIcon")

table.table
  thead
    tr
      th Month
      th Clear
      th Partly Cloudy
      th Cloudy
  tbody
    tr(ng-repeat="(name, types) in data")
      td {{name}}
      td {{types.clear}}
      td {{types.partlyCloudy}}
      td {{types.cloudy}}
